{ "name": "web-demos-client"
, "version": "0.1.0"
, "description": "client for the inference server"
, "homepage": "https://centtech.github.com/Neural/web-demos/blob/master/README.md"
, "repository":
  { "type": "git"
  , "url": "https://centtech.github.com/Neural/web-demos.git"
  }
, "license": "Apache-2.0"
, "private": true
, "source": "src/index.ts"
, "module": "dist/client.esm.js"
, "types": "dist/src/index.d.ts"
, "files":
  [ "dist" ]
, "peer-dependencies":
  { "@tensorflow/tfjs": "~1.2.7" }
, "dependencies":
  { "protobufjs": "~6.8.8"
  , "lodash-es": "~4.17.15"
  , "@tensorflow/tfjs": "~1.2.7"
  , "cross-fetch": "~3.0.4"
  }
, "devDependencies":
  { "typescript": "~3.5.3"
  , "tslint": "~5.18.0"
  , "ts-node": "~8.3.0"
  , "rollup": "~1.19.4"
  , "rollup-plugin-typescript2": "~0.22.1"
  , "rollup-plugin-node-resolve": "~5.2.0"
  , "rollup-plugin-node-builtins": "~2.1.2"
  , "rollup-plugin-commonjs": "~10.0.2"
  , "rollup-plugin-sourcemaps": "~0.4.2"
  , "rollup-plugin-replace": "~2.2.0"
  , "typescript-formatter": "~7.2.2"
  , "jest": "~24.9.0"
  , "ts-jest": "~24.0.2"
  , "babel-jest": "~24.9.0"
  , "@babel/preset-env": "~7.5.5"
  , "@babel/plugin-transform-modules-commonjs": "~7.5.0"
  , "@types/jest": "~24.0.18"
  , "@types/lodash-es": "~4.17.3"
  , "@types/node": "~12.7.2"
  , "cross-env": "~5.2.0"
  , "coveralls": "~3.0.6"
  }
, "scripts":
  { "build-messages":
      "cross-env TS_NODE_PROJECT=scripts/tsconfig.json ts-node ./scripts/build-messages.ts"
  , "build": "rollup -c"
  , "watch": "rollup -cw"
  , "lint": "tslint -p . -t stylish"
  , "format": "tsfmt -r && tslint -p . -t stylish --fix"
  , "test": "jest"
  , "check-formatting": "tsfmt --verify"
  , "coveralls": "coveralls"
  }
, "jest":
  { "verbose": true
  , "testEnvironment": "jest-environment-node"
  , "rootDir": "."
  , "collectCoverage": true
  , "coverageDirectory": "<rootDir>/build/coverage"
  , "coverageReporters":
    [ "json"
    , "lcov"
    , "text"
    , "clover"
    ]
  , "collectCoverageFrom":
    [ "<rootDir>/src/**/*.{ts,tsx}" ]
  , "transform":
    { "^.+\\.tsx?$": "ts-jest"
    , "^.+\\.jsx?$":
      [ "babel-jest"
      , { "plugins":
          [ "@babel/plugin-transform-modules-commonjs"]
        }
      ]
    }
  , "transformIgnorePatterns":
    [ "/!<rootDir>\\/node_modules\\/lodash-es/" ]
  , "testMatch":
    [ "<rootDir>/tests/**/*.{ts,tsx}" ]
  , "moduleFileExtensions":
    [ "ts"
    , "tsx"
    , "js"
    , "jsx"
    , "json"
    ]
  , "globals":
    { "ts-jest":
      { "tsConfig": "<rootDir>/tests/tsconfig.json" }
    }
  }
, "config":
  { "message_build_folder": "build"
  , "message_types_folder": "build"
  }
}
